{
    "projects": [
      {
        "src": "viperr-frontend",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      },
      {
        "src": "api",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/api/$1" }
    ]
  }